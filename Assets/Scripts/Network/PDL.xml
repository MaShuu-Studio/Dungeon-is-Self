<?xml version="1.0" encoding="utf-8" ?>
<PDL>
	<packet name="S_BroadcastConnectUser">
		<int name="totalUser"/>
		<int name="playingUser"/>
		<int name="waitDefUser"/>
		<int name="waitOffUser"/>
	</packet>
	<packet name="C_EnterGame">
	</packet>
	<packet name="S_GivePlayerId">
		<int name="playerId"/>
	</packet>
	<packet name="C_LeaveGame">
		<int name="playerId"/>
	</packet>
	<packet name="C_SingleGameRequest">
		<int name="playerId"/>
		<ushort name="playerType"/>
	</packet>
	<packet name="C_MatchRequest">
		<int name="playerId"/>
		<ushort name="playerType"/>
	</packet>
	<packet name="C_MatchRequestCancel">
		<int name="playerId"/>
		<ushort name="playerType"/>
	</packet>
	<packet name="S_StartGame">
		<int name="roomId"/>
		<int name="enemyPlayerId"/>
		<ushort name="playerType"/>
	</packet>
	<packet name="C_ReadyGame">
		<int name="roomId"/>
		<ushort name="playerType"/>
		<list name="candidate">
			<int name="int"/>
		</list>
	</packet>
	<packet name="S_ReadyGameEnd">
		<ushort name="currentProgress"/>
		<int name="round"/>
		<list name="enemyCandidate">
			<int name="int"/>
		</list>
	</packet>
	<packet name="C_RoundReady">
		<int name="roomId"/>
		<ushort name="playerType"/>
		<clist name="roster">
			<int name="unitIndex"/>
			<int name="attackSkill"/>
			<list name="skillRoster">
				<int name="int"/>
			</list>
		</clist>
	</packet>
	<packet name="S_RoundReadyEnd">
		<ushort name="currentProgress"/>
		<int name="round"/>
		<clist name="enemyRoster">
			<int name="unitIndex"/>
			<list name="skillRoster">
				<int name="int"/>
			</list>
		</clist>
	</packet>
	<packet name="C_PlayRoundReady">
		<int name="roomId"/>
		<ushort name="playerType"/>
		<clist name="roster">
			<int name="unitIndex"/>
			<list name="diceIndex">
				<int name="int"/>
			</list>
		</clist>
	</packet>
	<packet name="S_ProgressTurn">
		<bool name="isRoundEnd"/>
		<bool name="isGameEnd"/>
		<ushort name="winner"/>
		<int name="round"/>
		<int name="turn"/>
		<list name="monsterHp">
			<int name="int"/>
		</list>
		<int name="monsterTurn"/>
		<clist name="result">
			<int name="unitIndex"/>
			<int name="diceResult"/>
			<list name="diceIndex">
				<int name="int"/>
			</list>
		</clist>
	</packet>
</PDL>