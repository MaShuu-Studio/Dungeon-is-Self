<?xml version="1.0" encoding="utf-8" ?>
<PDL>
	<packet name="S_BroadcastConnectUser">
		<int name="totalUser"/>
		<int name="playingUser"/>
		<int name="waitDefUser"/>
		<int name="waitOffUser"/>
	</packet>
	<packet name="C_EnterGame">
	</packet>
	<packet name="S_GivePlayerId">
		<int name="playerId"/>
	</packet>
	<packet name="C_LeaveGame">
		<int name="playerId"/>
	</packet>
	<packet name="C_MatchRequest">
		<int name="playerId"/>
		<ushort name="playerType"/>
	</packet>
	<packet name="C_MatchRequestCancel">
		<int name="playerId"/>
		<ushort name="playerType"/>
	</packet>
	<packet name="S_StartGame">
		<int name="roomId"/>
		<int name="enemyPlayerId"/>
		<ushort name="playerType"/>
	</packet>
	<packet name="C_ReadyGame">
		<int name="playerId"/>
		<ushort name="playerType"/>
		<list name="candidate">
			<int name="int"/>
		</list>
	</packet>
	<packet name="S_GameState">
		<ushort name="currentProgress"/>
		<int name="round"/>
	</packet>
	<packet name="C_RoundReadyEnd">
		<clist name="roster">
			<int name="unitIndex"/>
			<list name="skillRosters">
				<int name="int"/>
			</list>
		</clist>
	</packet>
	<packet name="C_PlayRoundReady">
		<clist name="roster">
			<int name="unitIndex"/>
			<list name="diceIndex">
				<int name="int"/>
			</list>
		</clist>
	</packet>
	<packet name="S_ProgressTurn">
		<bool name="isRoundEnd"/>
		<bool name="isGameEnd"/>
		<ushort name="winner"/>
		<int name="turn"/>
		<int name="monsterHp"/>
		<int name="monsterTurn"/>
		<clist name="result">
			<int name="unitIndex"/>
			<list name="diceIndex">
				<int name="int"/>
			</list>
			<clist name="cc">
				<int name="ccIndex"/>
				<int name="ccTurn"/>
				<int name="ccStack"/>
			</clist>
		</clist>
	</packet>
</PDL>