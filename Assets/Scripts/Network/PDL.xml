<?xml version="1.0" encoding="utf-8" ?>
<PDL>
	<packet name="S_BroadcastEnterGame">
		<int name="playerId"/>
		<float name="xPos"/>
		<float name="yPos"/>
		<float name="zPos"/>
	</packet>
	<packet name="C_LeaveGame">
		<int name="playerId"/>
	</packet>
	<packet name="S_BroadcastLeaveGame">
		<int name="playerId"/>
	</packet>
	<packet name="S_PlayerList">
		<list name="player"> 
			<bool name="isSelf"/>
			<int name="playerId"/>
			<float name="xPos"/>  
			<float name="yPos"/>
			<float name="zPos"/>
		</list>
	</packet>
	<packet name="C_Move">
		<float name="xPos"/>
		<float name="yPos"/>
		<float name="zPos"/>
	</packet>
	<packet name="S_BroadcastMove">
		<int name="playerId"/>
		<float name="xPos"/>
		<float name="yPos"/>
		<float name="zPos"/>
	</packet>
</PDL>