<?xml version="1.0" encoding="utf-8" ?>
<PDL>
	<packet name="C_CheckConnect">
		<string name="playerId"/>
	</packet>
	<packet name="S_CheckConnect">
	</packet>
	<packet name="S_BroadcastConnectUser">
		<int name="totalUser"/>
		<int name="playingUser"/>
		<int name="waitDefUser"/>
		<int name="waitOffUser"/>
	</packet>
	<packet name="C_EnterGame">
	</packet>
	<packet name="S_GivePlayerId">
		<string name="playerId"/>
	</packet>
	<packet name="C_LeaveGame">
		<string name="roomId"/>
		<string name="playerId"/>
	</packet>
	<packet name="C_SingleGameRequest">
		<string name="playerId"/>
		<ushort name="playerType"/>
	</packet>
	<packet name="C_MatchRequest">
		<string name="playerId"/>
		<ushort name="playerType"/>
	</packet>
	<packet name="C_MatchRequestCancel">
		<string name="playerId"/>
		<ushort name="playerType"/>
	</packet>
	<packet name="S_StartGame">
		<string name="roomId"/>
		<string name="enemyPlayerId"/>
		<ushort name="playerType"/>
	</packet>
	<packet name="C_ReadyGame">
		<string name="roomId"/>
		<ushort name="playerType"/>
		<list name="candidate">
			<int name="int"/>
		</list>
	</packet>
	<packet name="S_ReadyGameEnd">
		<ushort name="currentProgress"/>
		<int name="round"/>
		<list name="enemyCandidate">
			<int name="int"/>
		</list>
	</packet>
	<packet name="C_RoundReady">
		<string name="roomId"/>
		<ushort name="playerType"/>
		<clist name="roster">
			<int name="unitIndex"/>
			<int name="attackSkill"/>
			<list name="skillRoster">
				<int name="int"/>
			</list>
		</clist>
	</packet>
	<packet name="S_RoundReadyEnd">
		<ushort name="currentProgress"/>
		<int name="round"/>
		<clist name="enemyRoster">
			<int name="unitIndex"/>
			<list name="skillRoster">
				<int name="int"/>
			</list>
			<int name="attackSkill"/>
		</clist>
	</packet>
	<packet name="C_PlayRoundReady">
		<string name="roomId"/>
		<ushort name="playerType"/>
		<clist name="roster">
			<int name="unitIndex"/>
			<list name="diceIndex">
				<int name="int"/>
			</list>
		</clist>
	</packet>
	<packet name="S_ProgressTurn">
		<bool name="isGameEnd"/>
		<ushort name="winner"/>
		<int name="endTurn"/>
		<int name="round"/>
		<int name="turn"/>
		<int name="resetTurn"/>
		<list name="monsterHp">
			<int name="int"/>
		</list>
		<int name="monsterTurn"/>
		<clist name="result">
			<bool name="isDead"/>
			<int name="deadTurn"/>
			<int name="unitIndex"/>
			<int name="diceResult"/>
			<list name="diceIndex">
				<int name="int"/>
			</list>
			<clist name="ccWithTurn">
				<clist name="cc">
					<int name="ccid"/>
					<int name="ccturn"/>
					<int name="ccstack"/>
					<bool name="ccOn"/>
				</clist>
			</clist>
		</clist>
	</packet>
	<packet name="C_RoundEnd">
		<string name="roomId"/>
		<ushort name="type"/>
	</packet>
	<packet name="S_NewRound">
		<int name="round"/>
		<clist name="userInfo">
			<ushort name="type"/>
			<list name="deadUnit">
				<int name="int"/>
			</list>
		</clist>
	</packet>
	<packet name="S_Timeout">
		<int name="time"/>
		<ushort name="currentProgress"/>
	</packet>
	<packet name="C_ReadyCancel">
		<string name="roomId"/>
		<ushort name="userType"/>
	</packet>	
	<packet name="C_GameEnd">
		<string name="roomId"/>
		<ushort name="type"/>
	</packet>
	<packet name="S_GameEnd">
		<ushort name="winner"/>
		<string name="winnerId"/>
	</packet>
</PDL>